<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.sherwin.opengles">

    <application
        android:allowBackup="true"
        android:name=".app.GlobalApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".shape.first.FirstActivity"></activity>
        <activity android:name=".shape.triangle.TriangleActivity"></activity>
        <activity android:name=".shape.Square.SquareActivity"></activity>
        <activity android:name=".shape.circle.CircleActivity"></activity>
        <activity android:name=".shape.cube.CubeActivity"></activity>

    </application>

</manifest>